{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],

  "buildCommand": "cd shared && npm run build && cd ../frontend && npm run build",
  "routes": [{ "src": "/(.*)", "dest": "frontend/dist/$1" }]
}
